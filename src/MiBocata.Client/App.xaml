<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:converters="clr-namespace:MiBocata.Converters"
            xmlns:local="clr-namespace:MiBocata"
             x:Class="MiBocata.App">
    <Application.Resources>
        <ResourceDictionary Source="Resources/Styles.xaml" />
        <ResourceDictionary>
            <converters:NegateBooleanConverter x:Key="NegateBoolConverter" />
            <converters:TextOrderStateConverter x:Key="TextOrderStateConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>