<?xml version="1.0" encoding="utf-8" ?>
<Application
    x:Class="MiBocata.Businnes.App"
   xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:converters="clr-namespace:MiBocata.Businnes.Converters">
    <Application.Resources>
        <ResourceDictionary Source="Resources/Styles.xaml" />
        <ResourceDictionary>
            <converters:NegateBooleanConverter x:Key="NegateBoolConverter" />
            <converters:AcceptedOrderStateConverter x:Key="AcceptedOrderStateConverter" />
            <converters:TextOrderStateConverter x:Key="TextOrderStateConverter" />
            <converters:TextColorStateConverter x:Key="TextColorStateConverter" />
            <converters:NegateIsNullOrEmptyConverter x:Key="NegateIsNullOrEmptyConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>